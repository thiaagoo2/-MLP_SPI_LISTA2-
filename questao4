import javax.swing.JOptionPane; 
public class Temperatura {
	public static void main(String[] args) {

		String f = JOptionPane.showInputDialog("Forneça a temperatura em graus Fahrenheit");
		int F = Integer.parseInt(f);
		int c = (F - 32) * 5 /9;
		JOptionPane.showMessageDialog (null, "A temperatura graus Celsius é: "+ c, f, JOptionPane.PLAIN_MESSAGE );
				System.exit( 0 );
		
	}
}
